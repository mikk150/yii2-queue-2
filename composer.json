{
    "name": "yiisoft/yii2-queue",
    "description": "Yii2 Queue Extension",
    "type": "yii2-extension",
    "keywords": ["yii", "queue"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roman Zhuravlev",
            "email": "zhuravljov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "~2.0.10",
        "kraken-php/promise": "^0.4.1",
        "react/child-process": "^0.5.0",
        "react/event-loop": "0.4.*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",

        "phpunit/phpunit": "~4.4",
        "codeception/codeception": "~2.2.11",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "yii\\queue\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}